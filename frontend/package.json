{
  "name": "barracao-digital",
  "version": "1.3.2",
  "description": "Estruturação rápida de “postos de triagem” virtuais descentralizados para redução de visitas desnecessárias às emergências",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build -a",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:open:wsl": "DISPLAY=localhost:0.0 cypress open",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 test:e2e"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
