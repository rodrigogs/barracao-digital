    "@nuxtjs/vuetify": "^1.11.2",
    "@rodrigogs/promise-pool": "^1.0.3",
    "apexcharts": "^3.19.3",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.15.0",
    "firebase": "^7.17.1",
    "idle-js": "^1.2.0",
