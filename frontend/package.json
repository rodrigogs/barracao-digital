    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "firebase-admin": "^9.0.0",
    "husky": "^4.2.5",
    "jest": "^26.2.1",
    "lint-staged": "^10.2.11",
    "nuxt-twa-module": "^1.3.1",
    "prettier": "^2.0.5",
